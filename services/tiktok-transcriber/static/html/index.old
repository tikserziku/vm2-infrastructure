<script>
// Mobile Admin Functions
let mobileMenuOpen = false;

function toggleMobilePanel() {
    const menu = document.getElementById('admin-menu');
    const fab = document.getElementById('fab-button');
    
    mobileMenuOpen = !mobileMenuOpen;
    
    if (mobileMenuOpen) {
        menu.style.display = 'block';
        fab.classList.add('active');
        // Check status when opening
        mobileCheckStatus();
    } else {
        menu.style.display = 'none';
        fab.classList.remove('active');
    }
}

async function mobileRestart() {
    if (!confirm('–ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∏—Ç—å —Å–µ—Ä–≤–∏—Å TikTok Transcriber?')) {
        return;
    }
    
    showMobileMessage('–ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞...', 'warning');
    
    try {
        const response = await fetch('/admin/restart', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            }
        });
        
        if (response.ok) {
            showMobileMessage('‚úÖ –ü–µ—Ä–µ–∑–∞–≥—Ä—É–∂–µ–Ω–æ!', 'success');
            setTimeout(() => {
                location.reload();
            }, 2000);
        } else {
            throw new Error('Failed to restart');
        }
    } catch (error) {
        showMobileMessage('‚ùå –û—à–∏–±–∫–∞!', 'danger');
        console.error('Restart error:', error);
    }
}

async function mobileCheckStatus() {
    try {
        const response = await fetch('/admin/status');
        
        if (response.ok) {
            const data = await response.json();
            const statusBadge = document.getElementById('service-status');
            
            if (data.status === 'success') {
                statusBadge.className = 'badge bg-success';
                statusBadge.textContent = 'Online';
                showMobileMessage('‚úÖ –°–µ—Ä–≤–∏—Å —Ä–∞–±–æ—Ç–∞–µ—Ç', 'success');
            } else {
                statusBadge.className = 'badge bg-danger';
                statusBadge.textContent = 'Offline';
            }
        }
    } catch (error) {
        document.getElementById('service-status').className = 'badge bg-warning';
        document.getElementById('service-status').textContent = 'Unknown';
    }
}

function mobileClearCache() {
    // Clear caches
    if ('caches' in window) {
        caches.keys().then(names => {
            names.forEach(name => caches.delete(name));
        });
    }
    
    // Clear storage
    localStorage.clear();
    sessionStorage.clear();
    
    showMobileMessage('‚úÖ –ö—ç—à –æ—á–∏—â–µ–Ω!', 'success');
    
    setTimeout(() => {
        location.reload(true);
    }, 1500);
}

function showMobileMessage(message, type) {
    const msgDiv = document.getElementById('mobile-message');
    const alertDiv = msgDiv.querySelector('.alert');
    
    alertDiv.className = `alert alert-${type} alert-sm p-2 mb-0`;
    alertDiv.textContent = message;
    alertDiv.style.fontSize = '12px';
    msgDiv.style.display = 'block';
    
    setTimeout(() => {
        msgDiv.style.display = 'none';
    }, 3000);
}

// Close menu when clicking outside
document.addEventListener('click', (e) => {
    const panel = document.getElementById('mobile-admin-panel');
    if (!panel.contains(e.target) && mobileMenuOpen) {
        toggleMobilePanel();
    }
});

// Touch gesture support for mobile
let touchStartY = 0;
document.getElementById('fab-button').addEventListener('touchstart', (e) => {
    touchStartY = e.touches[0].clientY;
});

document.getElementById('fab-button').addEventListener('touchend', (e) => {
    const touchEndY = e.changedTouches[0].clientY;
    if (Math.abs(touchEndY - touchStartY) < 10) {
        // It's a tap, not a swipe
        e.preventDefault();
    }
});
</script>

